
{
  "manifest_version": 2,

  "name": "Reorder Google Nav",
  "description": "Allows users customizing fixed order for google nav items (no customizing at this moment)",
  "version": "0.0.1",

  "browser_action": {
    "default_title": "Reorder Google Nav",
    "default_icon": "icon.png"
  },
  "content_scripts": [{
    "js": ["jquery-3.3.1.min.js", "URI.min.js", "content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "include_globs":  ["http://www.google.*/*", "https://www.google.*/*"]
  }]
}