
{
  "manifest_version": 2,

  "name": "Rearrange google nav items order",
  "description": "This extension allows the user to customized google nav items order",
  "version": "1.0",

  "browser_action": {
    "default_title": "Rearrange google nav items order",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "include_globs":  ["http://www.google.*/*", "https://www.google.*/*"]
  }]
}