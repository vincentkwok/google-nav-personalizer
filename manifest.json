
{
  "manifest_version": 2,

  "name": "Google Nav Personalizer",
  "description": "Allows users customizing order for google nav items (fixed order at this moment)",
  "version": "0.0.1",

  "browser_action": {
    "default_title": "Google Nav Personalizer",
    "default_icon": {
      "16": "./images/icon16.png",
      "48": "./images/icon48.png",
      "128": "./images/icon128.png"
    },
    "default_popup": ""
  },
  "content_scripts": [{
    "js": ["./libs/jquery-3.3.1.min.js", "./libs/URI.min.js", "./scripts/content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "include_globs":  ["http://www.google.*/*", "https://www.google.*/*"]
  }]
}
