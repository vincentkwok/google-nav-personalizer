
{
  "manifest_version": 2,

  "description": "This extension allows the user to customized google nav items order",
  "name": "Reorder Google Nav",
  "version": "0.0.1",

  "browser_action": {
    "default_title": "Reorder Google Nav",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["jquery-3.3.1.min.js", "URI.min.js", "content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "include_globs":  ["http://www.google.*/*", "https://www.google.*/*"]
  }]
}